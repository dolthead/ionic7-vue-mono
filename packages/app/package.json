{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite dev",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "ionic:serve": "vite dev",
    "ionic:build": "vue-tsc --noEmit && vite build",
    "e2e": "cypress run",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "serve:pwa": "vite build --mode pwa && vite preview --host --mode pwa",
    "serve:dist": "vite preview",
    "assets": "npx @capacitor/assets generate --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'"
  },
  "dependencies": {
    "@capacitor/app": "4.0.1",
    "@capacitor/core": "4.2.0",
    "@capacitor/haptics": "4.0.1",
    "@capacitor/keyboard": "4.0.1",
    "@capacitor/status-bar": "4.0.1",
    "@ionic/core": "^7.0.0",
    "@ionic/vue": "^7.0.0",
    "@ionic/vue-router": "^7.0.0",
    "@vueuse/core": "^9.13.0",
    "@vueuse/integrations": "^9.13.0",
    "ionicons": "^7.0.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "4.2.0",
    "@types/node": "^18.15.10",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/compiler-sfc": "3.2.47",
    "prettier": "^2.8.4",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  }
}
